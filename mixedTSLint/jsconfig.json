{
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonJS",
    "target": "es2020",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "maxNodeModuleJsDepth": 0,
    "moduleResolution": "Node",
    "checkJs": true
  },
  "exclude": [
    "node_modules",
    "app/browserFiles"
  ]
}