{
  "compilerOptions": {
    "lib": ["ES2015","DOM"],
    "module": "AMD",
    "target": "ES2015",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "maxNodeModuleJsDepth": 0,
    "moduleResolution": "Node",
    "outFile": "dist/script.js"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}